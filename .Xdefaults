! $OpenBSD: dot.Xdefaults,v 1.3 2014/07/10 10:22:59 jasper Exp $
XTerm*loginShell:true

!====fonts
Xft.autohint:0
Xft.lcdfilter:lcddefault
Xft.hintstyle:hintslight
Xft.hinting:1
Xft.antialias:1
Xft.rgba:rgb


!*font:
!*font:JetBrainsMonoBol:size=13

!XTerm.vt100.faceName:JetBrainsMonoMed:size=13
!XTerm.vt100.faceName: Spleen 12x24:style=Medium:size=15
XTerm.vt100.faceName: JetBrains Mono:style=Medium:size=12
!XTerm.vt100.font: 8x16
!XTerm*borderWidth:0
XTerm*internalBorder:2
XTerm*termName:xterm-256color
XTerm.vt100.saveLines:10000
XTerm.vt100.scrollBar:false
XTerm.vt100.geometry:90x35
XTerm.vt100.metaSendsEscape:true
XTerm.vt100.backarrowKey:false
!XTerm.vt100.translations: #override \n\
!	Ctrl <Key>M: maximize() \n\
!	Ctrl <Key>R: restore()
XTerm.ttyModes: erase ^?
XTerm*allowBoldFonts:false
!XTerm*scrollKey:true
!XTerm*fullscreen:never
XTerm*on2Clicks:word
XTerm*on3Clicks:line
!XTerm*scrollBar:false
XTerm*selectToClipboard:true
xterm.vt100.translations: #override \n\
	Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
	Ctrl Shift <Key>V: insert-selection(CLIPBOARD)
XTerm*background:#000000
XTerm*foreground:#ffffff
